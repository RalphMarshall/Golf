(defun golf-write-zero-rectangle (w h)
  (save-excursion
    (let ((num-lines h))
      (while (> num-lines 0)
        (insert-char 49 w)
        (newline)
        (setq num-lines (1- num-lines))))))

(defun golf-write-identicon-background ()
  (interactive)
  (golf-write-zero-rectangle 100 100))

(defun golf-replace-triangle (start-column height top-width bottom-width quadrant)
  (save-excursion
    (let* ((line-delta (/ (float (- bottom-width top-width)) (1- height)))
           (cur-width top-width))
      (while (> height 0)
        (move-to-column start-column)
        (delete-char (round cur-width))
        (insert-char 48 cur-width)
        (forward-line 1)
        (setq height (1- height))
        (setq cur-width (+ cur-width line-delta))
        ))))

(defun test-so-far ()
  (interactive)
  (delete-region 0 (point-max))
  (golf-write-zero-rectangle 20 30)
  (golf-replace-triangle 5 11 1 11 1))


